{
    "name": "omarelhawary/pdf-invoice-generator-for-memberpress",
    "description": "A WordPress plugin for generating PDF invoices for MemberPress transactions",
    "type": "wordpress-plugin",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "Omar ElHawary",
            "email": "contact@omarelhawary.com"
        }
    ],
    "require": {
        "php": ">=7.4"
    },
    "require-dev": {
        "wp-coding-standards/wpcs": "^3.0",
        "phpcompatibility/phpcompatibility-wp": "*",
        "dealerdirect/phpcodesniffer-composer-installer": "^1.0"
    },
    "scripts": {
        "cs-check": "phpcs --standard=WordPress .",
        "cs-fix": "phpcbf --standard=WordPress ."
    },
    "minimum-stability": "stable"
}
